Package: familiar
Title: End-to-End Automated Machine Learning and Model Evaluation
Version: 1.2.0
Authors@R: c(
    person("Alex", "Zwanenburg",
           email = "alexander.zwanenburg@nct-dresden.de",
           role = c("aut", "cre"),
           comment = c(ORCID = "0000-0002-0342-9545")),
